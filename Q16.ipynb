   "source": [
    "N = 4\n",
    "\n",
    "with open('popular-names.txt', 'r') as p:\n",
    "    lines = p.readlines()\n",
    "\n",
    "devider = len(lines) // N\n",
    "\n",
    "for i in range(N):\n",
    "    with open('output'+ str(i) + '.txt', 'w') as o:\n",
    "        o.write(''.join(lines[i * devider : (i+1) * devider))\n"
   ]
